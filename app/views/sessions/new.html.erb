<div class="mid">
    <div class="login_head_box"><img src="img/1-Loging-Page_03.png" width="71" height="51" style="margin:0 0 0 15px;" /></div>
        <div class="spacer"></div>
        <div class="wel_head">Welcome to Eeyes systems</div>
        <div class="spacer1"></div>
        <div class="login_box">
            <div class="login_box_head"><img src="img/1-Loging-Page_0.png" width="124" height="30" style="margin:14px 0 0 14px;" /></div>
            <div class="login_box_two">
                <%= form_tag new_session_path do %>
                <table width="100%" height="128" border="0">
                    <tr>
                        <td width="2%">&nbsp;</td>
                        <td width="28%" align="right">User Name :</td>
                        <td width="5%">&nbsp;</td>
                        <td width="65%"><%= text_field_tag :email, params[:email], :style => 'width:234px; height:30px; border:#999999 1px solid;' %></td>
                    </tr>
                    <tr>
                        <td>&nbsp;</td>
                        <td align="right">Password :</td>
                        <td width="5%">&nbsp;</td>
                        <td><%= password_field_tag :password, "", :style => 'width:234px; height:30px; border:#999999 1px solid;' %></td>
                    </tr>
                    <tr>
                        <td>&nbsp;</td>
                        <td></td>
                        <td width="5%">&nbsp;</td>
                        <td><input name="commit" type="image" src="img/1-Loging-Page_07.png" width="67" height="29" />&nbsp;&nbsp;&nbsp;<%= link_to "Forgot your password?", new_password_reset_path %></td>
                    </tr>
                </table>
                <% end %>
            </div>
        </div>
    </div>

